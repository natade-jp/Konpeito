{
	"name": "konpeito",
	"version": "2.0.0",
	"description": "The collection of javascirpt library such as numerical calculation.",
	"author": "natade-jp <natade3@gmail.com> (https://github.com/natade-jp)",
	"repository": {
		"type": "git",
		"url": "https://github.com/natade-jp/konpeito.git"
	},
	"license": "MIT",
	"main": "./build/CommonJS",
	"module": "./build/esm",
	"keywords": [
		"math",
		"mathematics",
		"biginteger",
		"bigdecimal",
		"bignumber",
		"fraction",
		"complex",
		"matrix",
		"algebra",
		"statistics",
		"fft",
		"svd"
	],
	"directories": {
		"src": "./src",
		"build": "./build",
		"example": "./html/examples/demos"
	},
	"scripts": {
		"dev": "node ./scripts/package.dev.js",
		"build": "node ./scripts/package.build.js",
		"doc": "node ./scripts/package.doc.js",
		"dts": "node ./scripts/package.dts.js",
		"test": "jest",
		"test_target": "node ./scripts/package.test.js"
	},
	"jest": {
		"verbose": true,
		"rootDir": "./src",
		"moduleFileExtensions": [
			"js",
			"mjs"
		],
		"testMatch": [
			"**/__tests__/**/*.?(m)js?(x)",
			"**/?(*.)(spec|test).?(m)js?(x)"
		],
		"transform": {
			"^.+\\.(js|mjs)$": "babel-jest"
		}
	},
	"babel": {
		"env": {
			"test": {
				"plugins": [
					"transform-es2015-modules-commonjs"
				]
			}
		}
	},
	"devDependencies": {
		"@types/jest": "^24.9.1",
		"babel-jest": "^24.9.0",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
		"esdoc": "^1.1.0",
		"esdoc-standard-plugin": "^1.0.0",
		"eslint": "^6.8.0",
		"eslint-plugin-jest": "^22.21.0",
		"jest": "^24.9.0",
		"jsdoc": "^3.6.3",
		"jsdoc-export-default-interop": "^0.3.1",
		"rollup": "^1.29.1",
		"rollup-plugin-buble": "^0.19.8",
		"rollup-plugin-uglify": "^6.0.4",
		"rollup-plugin-uglify-es": "0.0.1",
		"tsd-jsdoc": "^2.4.0"
	},
	"dependencies": {}
}
