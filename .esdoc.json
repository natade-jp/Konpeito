{
	"source": "./src",
	"destination": "./doc",
	"includes" : ["\\.(js|mjs)$"],
	"plugins": [
		{
			"name": "esdoc-standard-plugin",
			"option": {
				"unexportedIdentifier": {"enable": false},
				"manual": {
					"index": "./MANUAL.md",
					"files": [
						"./MANUAL.md"
					  ]
				}
			}
		}
	]
}
